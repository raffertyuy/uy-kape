{
  "name": "uy-kape",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --max-warnings 5",
    "preview": "vite preview",
    "test": "node --max-old-space-size=8192 ./node_modules/vitest/vitest.mjs",
    "test:run": "node --max-old-space-size=8192 ./node_modules/vitest/vitest.mjs run",
    "test:ui": "node --max-old-space-size=8192 ./node_modules/vitest/vitest.mjs --ui",
    "test:coverage": "node --max-old-space-size=8192 ./node_modules/vitest/vitest.mjs run --coverage",
    "test:watch": "node --max-old-space-size=8192 ./node_modules/vitest/vitest.mjs --watch",
    "test:ci": "node --max-old-space-size=10240 ./node_modules/vitest/vitest.mjs run --config=vitest.config.ci.ts --coverage --reporter=verbose --reporter=junit --outputFile=test-results.xml",
    "test:ci-no-coverage": "node --max-old-space-size=10240 ./node_modules/vitest/vitest.mjs run --config=vitest.config.ci.ts --reporter=verbose --reporter=junit --outputFile=test-results.xml",
    "test:memory": "node --max-old-space-size=10240 --expose-gc ./node_modules/vitest/vitest.mjs run --config=vitest.config.ci.ts --reporter=verbose --logHeapUsage"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.55.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.32",
    "supabase": "^2.34.3",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  }
}
