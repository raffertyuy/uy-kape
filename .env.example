# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Application Configuration
VITE_GUEST_PASSWORD=guest123
VITE_ADMIN_PASSWORD=admin456

# Guest Password Bypass Configuration
# Set to "true" to bypass guest password authentication
# When false or unset, password protection is required (default/production behavior)
VITE_GUEST_BYPASS_PASSWORD=false

# Order Wait Time Configuration
VITE_WAIT_TIME_PER_ORDER=4

# Error Handling Panel Configuration
VITE_ERROR_HANDLING_PANEL=false